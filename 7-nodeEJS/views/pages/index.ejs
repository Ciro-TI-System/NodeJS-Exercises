<!DOCTYPE html>
<html lang="pt-BR">

<%- include('../partials/head'); %>

  <body class="container">
    <%- include('../partials/header'); %>
      <main>
        <div class="jumbotron jumbotron-fluid text-center text-justify">
          <h1 class="display-4">Node com EJS</h1>
          <p class="lead">Vamos aprender a trabalhar com arquivos EJS.</p>
          <p>
            <%= subtitle %>
          </p>
        </div>
        <ul>
          <% qualitys.forEach(function(quality){ %>
            <li>
              <strong>
                <%= quality.title %>
              </strong>
              <%= quality.message %>
            </li>
            <% }) %>
        </ul>
      </main>
      <hr class="my-4" />

      <%- include('../partials/footer'); %>

  </body>

</html>